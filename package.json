    "next-themes": "^0.4.6",
    "openid-client": "^6.8.1",
    "plaiceholder": "^3.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-error-boundary": "^6.1.0",
    "react-parallax-tilt": "^1.7.315",
    "replace-in-file": "^8.4.0",
