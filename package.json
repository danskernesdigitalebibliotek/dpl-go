{
  "name": "go-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-storybook": "storybook build",
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "build:dev": "NEXT_PUBLIC_APP_URL=http://dapple-cms.docker:3000 next build",
    "ci-check": "yarn typecheck && yarn lint && yarn format:check && yarn test:unit:once",
    "codegen:all-rest-services": "orval",
    "codegen:fbs": "rm -rf src/lib/rest/fbs/generated/model/*.* && orval --project fbs",
    "codegen:graphql": "NODE_ENV=development DOTENV_CONFIG_PATH=./.env.local NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen -r dotenv/config --require tsconfig-paths/register --config codegen.ts",
    "codegen:publizon": "rm -rf lib/rest/publizon-api/generated/* && orval --project publizonAdapter && orval --project publizonLocalAdapter",
    "codegen:pubhub": "rm -rf lib/soap/publizon/v2_7/generated/* && npx tsx ./scripts/wsdl-tsclient/publizon-wsdl-codegen",
    "codegen:unilogin": "rm -rf lib/soap/unilogin/wsiinst-v5/generated/* && npx tsx ./scripts/wsdl-tsclient/unilogin-wsdl-codegen",
    "dev:test": "NODE_ENV=test next dev",
    "dev:https": "NEXT_PUBLIC_APP_URL=https://dapple-cms.docker:3000 NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -H dapple-cms.docker --experimental-https",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "start:with-server-source-maps": "NODE_OPTIONS='--enable-source-maps=true' next start",
    "start": "next start",
    "start:test": "NODE_ENV=test next start",
    "start:dev": "NEXT_PUBLIC_APP_URL=http://dapple-cms.docker:3000 NODE_TLS_REJECT_UNAUTHORIZED=0 next start",
    "storybook": "storybook dev -p 6006",
    "test:accessibility": "test-storybook",
    "test:unit:once": "vitest run",
    "test:unit": "vitest",
    "typecheck": "tsc --noEmit",
    "cypress:open": "DEBUG_MOCK_SERVER=true NODE_ENV=test cypress open --e2e",
    "cypress:open:mobile": "DEBUG_MOCK_SERVER=true NODE_ENV=test cypress open --e2e --env viewport=mobile",
    "cypress:run": "yarn cypress:run:mobile; yarn cypress:run:desktop",
    "cypress:run:mobile": "NODE_ENV=test cypress run --env viewport=mobile",
    "cypress:run:desktop": "NODE_ENV=test cypress run --env viewport=desktop",
    "shadcn:add": "npx shadcn@canary add $MODULE",
    "post-process-dpl-cms-graphql": "tsx ./scripts/graphql-codegen/post-process-dpl-cms-graphql.ts"
  },
  "dependencies": {
